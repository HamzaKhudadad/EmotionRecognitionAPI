    <section class="gray-bg section-padding" id="service-page">
      
                 
                      
        <div class="col-xs-12 col-sm-6 col-sm-offset-3 text-center">
            <div class="page-title">
                <h2>Upload your picture and get prediction</h2>
                
            </div>
        </div>


       
        <div class="container" >
            <div class="row">
                <div class="col-xs-12 col-sm-4 text-center">
                    <h4>Upload your audio</h4>
                    <form action="/pictureprediction" enctype="multipart/form-data" method="post">
                        <input type="file" name="upload" id="upload-input" >
                        <br>
                        <button type="submit" class="btn btn-primary btn-lg btn-block">Send</button>
                     </form>

                     


                </div>

                <div class="col-xs-12 col-sm-4 text-center">
                    <h4>Your uploaded picture</h4>
                     <img src="/files/<%=file %>"  style="height: 300px;">
                    
               

                    <p><%=file %></p><br>

                     

                </div>
                
                <div class="col-xs-12 col-sm-4 text-center">
                   

                     <h4>Your Facial Expression is</h4>
                    <h1> "<%=data %>"</h1> </div>

                </div>
            </div>
            
            
</section>
    











